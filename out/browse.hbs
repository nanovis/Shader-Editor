<div id="shaders_list">
</div>
<script type="text/javascript">
const _el = document.getElementById('shaders_list');
const _d = {{{data}}};
window.onload = () => {
_d.map((el) => {
  const obj = el.map((e) => {
    return `
     <div class="col-sm-6 col-md-3">
     <a href="/view_user?name=${e.name}&user=${e.user}">
     <img src="./view/${e.name}_${e.user}.png" alt="${e.name}" height="270" width="100%">
     </a>
     </div>
     `
  });
  const preVal = _el.innerHTML
  _el.innerHTML = (preVal + `
  <div class="row">
  ${obj.join('\n')}
  </div>
  <br>
  `)
})
}
</script>
